var webpPreloadCachePause = 1;
var currentProgress = 0;
var currentBtn = '';
var currentHtml = '';
  
function processQueue(start) {
  var webpPreloadCacheCurrentBtn = $('.webpPreloadCache');
  
  $.ajax({
    url: 'index.php?route=module/webp_image/process&start='+start+'&<?php echo $token; ?>',
    type: 'POST',
    //data: $('#update-form :input').serialize(),
    dataType: 'json',
    success: function(data){
      if(data.success) {
        $('.webpProgressBar .progress-bar').css('width',data.progress + '%').html(data.progress + ' %');
        $('.webpProgressBar .processProgress').html(data.progress + ' %');
        if (!webpPreloadCachePause && !data.finished) {
          processQueue(data.processed);
        } else {
          currentProgress = data.processed;
          $('.webpProgressBar .progress-bar').removeClass('active');
          webpPreloadCacheCurrentBtn.removeClass('webpPreloadCachePause').addClass('webpPreloadCache');
          $('button').attr('disabled', false);
          
          if (data.finished) {
            webpPreloadCacheCurrentBtn.html('<i class="fa fa-check"></i>');
            $('button').attr('disabled', false);
          }
        }
      }
    },
    error: function(xhr) {
      $('.webpProgressBar .progress-bar').css('width','100%');
      $('.webpProgressBar .webpPreloadCachePause').html('<i class="fa fa-check"></i> 100%');
      $('.webpProgressBar .progress-bar').removeClass('active');
      
      $('button').attr('disabled', false);
      webpPreloadCacheCurrentBtn.html('<i class="fa fa-exclamation-triangle"></i>');
      webpPreloadCacheCurrentBtn.removeClass('btn-success').addClass('btn-danger');
      webpPreloadCacheCurrentBtn.attr('disabled', true);
      alert(xhr.responseText);
    }
  });
}

var first_run = true;
  
$('body').on('click', '.webpPreloadCache', function(e) {
  e.stopPropagation();
  
  $('button').attr('disabled', true);
  
  $('.fa-pause').parent().removeClass('btn-success').addClass('btn-primary').html('<i class="fa fa-refresh"></i>');
  
  webpPreloadCacheCurrentBtn = $(this);
  
  currentHtml = webpPreloadCacheCurrentBtn.html();
  
  if (webpPreloadCacheCurrentBtn.html().indexOf('fa-pause') < 0) {
    webpPreloadCacheCurrentBtn.html('<i class="fa fa-refresh fa-spin"></i> Loading...');
  } else {
    webpPreloadCacheCurrentBtn.html(webpPreloadCacheCurrentBtn.html().replace('fa-pause', 'fa-refresh fa-spin'));
  }
  webpPreloadCacheCurrentBtn.removeClass('btn-primary').addClass('btn-success');
  webpPreloadCacheCurrentBtn.attr('disabled', false);
  webpPreloadCacheCurrentBtn.removeClass('webpPreloadCache').addClass('webpPreloadCachePause');
  
  $('.webpProgressBar .progress').show();
  $('.webpProgressBar .progress-bar').addClass('active');
  
  webpPreloadCachePause = 0;
  if (!currentProgress) {
    processQueue('init', webpPreloadCacheCurrentBtn.data('page'));
  } else {
    processQueue(currentProgress, webpPreloadCacheCurrentBtn.data('page'));
  }
});

$('body').on('click', '.webpPreloadCachePause', function(e) {
  e.stopPropagation();
  webpPreloadCachePause = 1;
  webpPreloadCacheCurrentBtn.html(webpPreloadCacheCurrentBtn.html().replace('fa-refresh fa-spin', 'fa-pause'));
});

</script>
